<h1>Edit Profile</h1>
<%= form_for @user, method: "PATCH" do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <br>
  <%=f.label :age%>
  <%=f.number_field :age%>
  <br>
  <%=f.label :username %>
  <%=f.text_field :username %>
  <br>
  <%=f.label :email%>
  <%=f.text_field :email%>
  <br>
  <%=f.label :password %>
  <%=f.password_field :password %>
  <br>
  <%=f.label :password_confirmation%>
  <%=f.password_field :password_confirmation%>
  <br>
  <%= f.submit "Submit" %>
<% end %>
